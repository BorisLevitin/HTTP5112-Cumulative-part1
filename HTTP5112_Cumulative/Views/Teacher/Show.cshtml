@model HTTP5112_Cumulative.Models.Teacher
@{
    ViewBag.Title = "Show";
}



<div id="show-container">
     <nav>
         <a class="nav-link" href="/Teacher/List">Back to Teachers</a>
         <a class="del-link" href="/Teacher/DeleteConfirm/@Model.TeacherId">Delete</a>

     </nav>
    <h2>@Model.TeacherFname @Model.TeacherLname</h2>
    <h3>Salary: $@Model.TeacherSalary</h3>
    <h3 style="font-style:italic">Hire Date: @Model.TeacherHireDate.ToString("dd-MMM-yyyy")</h3>
</div>

